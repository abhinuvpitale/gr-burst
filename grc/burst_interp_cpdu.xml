<?xml version="1.0"?>
<block>
  <name>Dynamic Interp/Synthesis</name>
  <key>burst_interp_cpdu</key>
  <category>burst</category>
  <import>import burst</import>
  <make>burst.interp_cpdu($fs)</make>
  <param>
    <name>Sample Rate</name>
    <key>fs</key>
    <value>samp_rate</value>
    <type>real</type>
  </param>
  <sink>
    <name>cpdus</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>cpdus</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
